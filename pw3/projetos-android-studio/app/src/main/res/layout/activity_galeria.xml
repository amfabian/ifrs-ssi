<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ScrollView
        android:layout_width="330dp"
        android:layout_height="479dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.769">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/cafecultura5"
                tools:layout_editor_absoluteX="125dp"
                tools:layout_editor_absoluteY="18dp"
                android:contentDescription="Café Cultura Shopping Iguatemi" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/cafecultura3"
                tools:layout_editor_absoluteX="125dp"
                tools:layout_editor_absoluteY="18dp"
                android:contentDescription="Café expresso do Café Cultura" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/cafecultura4"
                tools:layout_editor_absoluteX="125dp"
                tools:layout_editor_absoluteY="18dp"
                android:contentDescription="Café Cultura Shopping Iguatemi" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/cafecultura6"
                tools:layout_editor_absoluteX="125dp"
                tools:layout_editor_absoluteY="18dp"
                android:contentDescription="Capuccino Café Cultura" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:contentDescription="Café passado"
                app:srcCompat="@drawable/cafecultura2" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/keycoffee"
                tools:layout_editor_absoluteX="125dp"
                tools:layout_editor_absoluteY="18dp"
                android:contentDescription="Lata de Café Studio Key/VisualArts" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:contentDescription="Café passado"
                app:srcCompat="@drawable/coffee" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>